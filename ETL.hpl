            <conditions>
</conditions>
            <function>IS NOT NULL</function>
            <leftvalue>departamento</leftvalue>
            <negated>N</negated>
            <operator>AND</operator>
          </condition>
        </conditions>
        <function>IS NOT NULL</function>
        <leftvalue>datetime</leftvalue>
        <negated>N</negated>
        <operator>-</operator>
      </condition>
    </compare>
    <send_true_to>DateTime Lookup</send_true_to>
    <attributes/>
