<?xml version="1.0" encoding="UTF-8"?>
<pipeline>
  <info>
    <name>load_dimensions</name>
    <name_sync_with_filename>Y</name_sync_with_filename>
    <description>Carga todas las dimensiones</description>
    <extended_description/>
    <pipeline_version/>
    <pipeline_type>Normal</pipeline_type>
    <parameters>
    </parameters>
    <capture_transform_performance>N</capture_transform_performance>
    <transform_performance_capturing_delay>1000</transform_performance_capturing_delay>
    <transform_performance_capturing_size_limit>100</transform_performance_capturing_size_limit>
    <created_user>-</created_user>
    <created_date>2025/05/28 00:00:00.000</created_date>
    <modified_user>-</modified_user>
    <modified_date>2025/05/28 00:00:00.000</modified_date>
  </info>
  <notepads>
  </notepads>
  <order>
    <hop>
      <from>Generate DateTime</from>
      <to>DateTime Output</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Staging Conditions</from>
      <to>Condition Output</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Staging Stations</from>
      <to>Station Output</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Staging Locations</from>
      <to>Location Output</to>
      <enabled>Y</enabled>
    </hop>
  </order>

  <!-- Generar fechas usando SQL directo -->
  <transform>
    <name>Generate DateTime</name>
    <type>TableInput</type>
    <description>Genera fechas para la dimensión tiempo</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>postgress-datawarehouse</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT 
    date_series::date as date,
    date_series::timestamp as datetime,
    EXTRACT(YEAR FROM date_series) as year,
    EXTRACT(MONTH FROM date_series) as month,
    EXTRACT(DAY FROM date_series) as day,
    0 as hour,
    EXTRACT(DOW FROM date_series) as weekday,
    '06:00:00'::time as sunrise,
    '18:00:00'::time as sunset
FROM generate_series('2024-01-01'::date, '2024-12-31'::date, '1 day'::interval) as date_series;</sql>
    <variables_active>N</variables_active>
    <attributes/>
    <GUI>
      <xloc>96</xloc>
      <yloc>80</yloc>
    </GUI>
  </transform>

  <transform>
    <name>DateTime Output</name>
    <type>TableOutput</type>
    <description>Inserta fechas en DimDateTime</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <commit>1000</commit>
    <connection>postgress-datawarehouse</connection>
    <fields>
      <field>
        <column_name>datetime</column_name>
        <stream_name>datetime</stream_name>
      </field>
      <field>
        <column_name>date</column_name>
        <stream_name>date</stream_name>
      </field>
      <field>
        <column_name>year</column_name>
        <stream_name>year</stream_name>
      </field>
      <field>
        <column_name>month</column_name>
        <stream_name>month</stream_name>
      </field>
      <field>
        <column_name>day</column_name>
        <stream_name>day</stream_name>
      </field>
      <field>
        <column_name>hour</column_name>
        <stream_name>hour</stream_name>
      </field>
      <field>
        <column_name>weekday</column_name>
        <stream_name>weekday</stream_name>
      </field>
      <field>
        <column_name>sunrise</column_name>
        <stream_name>sunrise</stream_name>
      </field>
      <field>
        <column_name>sunset</column_name>
        <stream_name>sunset</stream_name>
      </field>
    </fields>
    <ignore_errors>N</ignore_errors>
    <only_when_have_rows>N</only_when_have_rows>
    <partitioning_daily>N</partitioning_daily>
    <partitioning_enabled>N</partitioning_enabled>
    <partitioning_monthly>Y</partitioning_monthly>
    <return_keys>N</return_keys>
    <schema>dw</schema>
    <specify_fields>Y</specify_fields>
    <table>"DimDateTime"</table>
    <tablename_in_field>N</tablename_in_field>
    <tablename_in_table>Y</tablename_in_table>
    <truncate>N</truncate>
    <use_batch>Y</use_batch>
    <attributes/>
    <GUI>
      <xloc>200</xloc>
      <yloc>80</yloc>
    </GUI>
  </transform>

  <!-- Cargar condiciones únicas del staging -->
  <transform>
    <name>Staging Conditions</name>
    <type>TableInput</type>
    <description>Obtiene condiciones únicas del staging</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>postgress-datawarehouse</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT DISTINCT 
    conditions as condition,
    COALESCE(description, conditions) as description,
    COALESCE(icon, 'default') as icon
FROM staging.clima 
WHERE conditions IS NOT NULL
ORDER BY conditions;</sql>
    <variables_active>N</variables_active>
    <attributes/>
    <GUI>
      <xloc>96</xloc>
      <yloc>160</yloc>
    </GUI>
  </transform>

  <transform>
    <name>Condition Output</name>
    <type>TableOutput</type>
    <description>Inserta en DimCondition</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <commit>1000</commit>
    <connection>postgress-datawarehouse</connection>
    <fields>
      <field>
        <column_name>condition</column_name>
        <stream_name>condition</stream_name>
      </field>
      <field>
        <column_name>description</column_name>
        <stream_name>description</stream_name>
      </field>
      <field>
        <column_name>icon</column_name>
        <stream_name>icon</stream_name>
      </field>
    </fields>
    <ignore_errors>N</ignore_errors>
    <only_when_have_rows>N</only_when_have_rows>
    <partitioning_daily>N</partitioning_daily>
    <partitioning_enabled>N</partitioning_enabled>
    <partitioning_monthly>Y</partitioning_monthly>
    <return_keys>N</return_keys>
    <schema>dw</schema>
    <specify_fields>Y</specify_fields>
    <table>"DimCondition"</table>
    <tablename_in_field>N</tablename_in_field>
    <tablename_in_table>Y</tablename_in_table>
    <truncate>N</truncate>
    <use_batch>Y</use_batch>
    <attributes/>
    <GUI>
      <xloc>200</xloc>
      <yloc>160</yloc>
    </GUI>
  </transform>

  <!-- Cargar estaciones únicas del staging -->
  <transform>
    <name>Staging Stations</name>
    <type>TableInput</type>
    <description>Obtiene estaciones únicas del staging</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>postgress-datawarehouse</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT DISTINCT 
    stations as code,
    stations as name,
    0.0 as lat,
    0.0 as lon,
    0.0 as elevation
FROM staging.clima 
WHERE stations IS NOT NULL
ORDER BY stations;</sql>
    <variables_active>N</variables_active>
    <attributes/>
    <GUI>
      <xloc>96</xloc>
      <yloc>240</yloc>
    </GUI>
  </transform>

  <transform>
    <name>Station Output</name>
    <type>TableOutput</type>
    <description>Inserta en DimStation</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <commit>1000</commit>
    <connection>postgress-datawarehouse</connection>
    <fields>
      <field>
        <column_name>code</column_name>
        <stream_name>code</stream_name>
      </field>
      <field>
        <column_name>name</column_name>
        <stream_name>name</stream_name>
      </field>
      <field>
        <column_name>lat</column_name>
        <stream_name>lat</stream_name>
      </field>
      <field>
        <column_name>lon</column_name>
        <stream_name>lon</stream_name>
      </field>
      <field>
        <column_name>elevation</column_name>
        <stream_name>elevation</stream_name>
      </field>
    </fields>
    <ignore_errors>N</ignore_errors>
    <only_when_have_rows>N</only_when_have_rows>
    <partitioning_daily>N</partitioning_daily>
    <partitioning_enabled>N</partitioning_enabled>
    <partitioning_monthly>Y</partitioning_monthly>
    <return_keys>N</return_keys>
    <schema>dw</schema>
    <specify_fields>Y</specify_fields>
    <table>"DimStation"</table>
    <tablename_in_field>N</tablename_in_field>
    <tablename_in_table>Y</tablename_in_table>
    <truncate>N</truncate>
    <use_batch>Y</use_batch>
    <attributes/>
    <GUI>
      <xloc>200</xloc>
      <yloc>240</yloc>
    </GUI>
  </transform>

  <!-- Cargar ubicaciones únicas del staging -->
  <transform>
    <name>Staging Locations</name>
    <type>TableInput</type>
    <description>Obtiene ubicaciones únicas del staging</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>postgress-datawarehouse</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT DISTINCT 
    departamento as name,
    'Colombia' as region,
    'CO' as country
FROM staging.clima 
WHERE departamento IS NOT NULL
ORDER BY departamento;</sql>
    <variables_active>N</variables_active>
    <attributes/>
    <GUI>
      <xloc>96</xloc>
      <yloc>320</yloc>
    </GUI>
  </transform>

  <transform>
    <name>Location Output</name>
    <type>TableOutput</type>
    <description>Inserta en DimLocation</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <commit>1000</commit>
    <connection>postgress-datawarehouse</connection>
    <fields>
      <field>
        <column_name>name</column_name>
        <stream_name>name</stream_name>
      </field>
      <field>
        <column_name>region</column_name>
        <stream_name>region</stream_name>
      </field>
      <field>
        <column_name>country</column_name>
        <stream_name>country</stream_name>
      </field>
    </fields>
    <ignore_errors>N</ignore_errors>
    <only_when_have_rows>N</only_when_have_rows>
    <partitioning_daily>N</partitioning_daily>
    <partitioning_enabled>N</partitioning_enabled>
    <partitioning_monthly>Y</partitioning_monthly>
    <return_keys>N</return_keys>
    <schema>dw</schema>
    <specify_fields>Y</specify_fields>
    <table>"DimLocation"</table>
    <tablename_in_field>N</tablename_in_field>
    <tablename_in_table>Y</tablename_in_table>
    <truncate>N</truncate>
    <use_batch>Y</use_batch>
    <attributes/>
    <GUI>
      <xloc>200</xloc>
      <yloc>320</yloc>
